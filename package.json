    "cors": "^2.8.5",
    "del-cli": "^3.0.1",
    "express": "^4.17.1",
    "firebase-admin": "^11.4.1",
    "firebase-functions": "^3.6.1"
  },
  "devDependencies": {
    "tslint": "^5.12.0",
    "typescript": "^3.8.0",
    "firebase-functions-test": "^0.2.0"
  },
  "private": true
